---
import { AstroSeo } from "@astrolib/seo";

import Layout from '../layouts/Layout.astro';
import PageContact from '../components/sections/PageContact.astro';
import PageHeader from '../components/sections/PageHeader.astro';
---
<AstroSeo
  title="Contact | WallyVac | Alberta Hydrovac Company"
  description="Reach out to WallyVac, your trusted hydrovac specialists in Edmonton, Alberta. We leverage cutting-edge technology and industry expertise to provide precise and reliable excavation solutions. Contact us today to discuss your project needs and receive a tailored quote."
  openGraph={{
    url: 'https://wallyvac.ca/',
    title: 'Contact | WallyVac | Alberta Hydrovac Company',
    description: 'Reach out to WallyVac, your trusted hydrovac specialists in Edmonton, Alberta. We leverage cutting-edge technology and industry expertise to provide precise and reliable excavation solutions. Contact us today to discuss your project needs and receive a tailored quote.',
    images: [
      {
        url: 'https://ik.imagekit.io/oy5bt2urx/Wally-Vac-Truck.webp',
        width: 800,
        height: 600,
        alt: 'Hydrovac truck',
        type: 'image/webp',
      },
    ],
    site_name: 'WallyVac | Alberta Hydrovac Company',
  }}
  />
<Layout>
	<PageHeader title="Contact"/>
	<PageContact/>
</Layout>
